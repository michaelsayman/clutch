{
  "name": "clutch-cli",
  "version": "1.0.0",
  "description": "AI-powered file description generator for GitHub repositories",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "bun src/index.ts",
    "compile": "bun build --compile --minify --sourcemap src/index.ts --outfile clutch",
    "compile:all": "bun build --compile --minify --sourcemap src/index.ts"
  },
  "keywords": [
    "cli",
    "ai",
    "documentation",
    "claude",
    "code-analysis"
  ],
  "author": "Michael Sayman",
  "license": "MIT",
  "dependencies": {
    "@inquirer/prompts": "^7.0.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "commander": "^12.0.0",
    "execa": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "bun-types": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
