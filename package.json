{
  "name": "clutch-cli",
  "version": "1.0.0",
  "description": "AI-powered file description generator for GitHub repositories",
  "main": "dist/index.js",
  "bin": {
    "clutch": "./dist/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "bundle": "esbuild dist/index.js --bundle --platform=node --target=node18 --outfile=dist/bundle.js --format=cjs --banner:js=\"#!/usr/bin/env node\" --external:fsevents --minify",
    "build:all": "npm run build && npm run bundle",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "prepare": "npm run build:all",
    "pkg": "npm run build:all && pkg dist/bundle.js --compress Brotli",
    "pkg:all": "npm run build:all && pkg dist/bundle.js --targets node18-macos-x64,node18-macos-arm64,node18-linux-x64,node18-linux-arm64 --compress Brotli --output clutch"
  },
  "keywords": [
    "cli",
    "ai",
    "documentation",
    "claude",
    "code-analysis"
  ],
  "author": "Michael Sayman",
  "license": "MIT",
  "dependencies": {
    "@inquirer/prompts": "^7.0.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "commander": "^12.0.0",
    "execa": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3",
    "pkg": "^5.8.1",
    "esbuild": "^0.20.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "pkg": {
    "targets": [
      "node18-macos-x64",
      "node18-macos-arm64",
      "node18-linux-x64",
      "node18-linux-arm64"
    ],
    "outputPath": "dist/bin",
    "assets": []
  }
}
